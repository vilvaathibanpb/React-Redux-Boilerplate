{
  "name": "cs-react-redux-boilerplate",
  "version": "1.0.0",
  "description": "A Basic reusable Boilerplate for React-Redux-Webpack-Router Web App",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "babel-build": "npm run build-server && npm run build-client",
    "babel-build-server": "babel server --out-dir ./dist",
    "babel-build-client": "babel src --copy-files --out-dir ./dist/public",
    "clean": "rimraf dist node_modules",
    "start": "npm run build && node ./dist/server.js",
    "test": "jest ./src",
    "server-test": "jest ./server"
  },
  "author": "Vilva Athiban P B",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "css-loader": "0.28.9",
    "cssnext": "1.8.4",
    "express": "^4.16.3",
    "postcss-loader": "2.0.5",
    "postcss-mixins": "6.2.0",
    "postcss-nested": "2.0.2",
    "postcss-import": "10.0.0",
    "postcss-cssnext": "3.1.0",
    "postcss-simple-vars": "4.1.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "style-loader": "0.12.3",
    "webpack": "^4.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "webpack-cli": "^2.0.14",
    "webpack-node-externals": "^1.7.2"
  }
}
